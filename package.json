{
  "name": "chat-davinci",
  "version": "1.0.0",
  "description": "End to end sample for running Node.js applications on Cloud Platform",
  "repository": "https://github.com/sabri1806/chat-davinci.git",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "test": "ava -t 30s test/*.test.js | tap-dot",
    "cover": "nyc --cache npm test; nyc report --reporter=html"
  },
  "author": "Sabrina Lucero",
  "license": "Apache-2.0",
  "semistandard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "~4.15.2",
    "socket.io": "^1.7.3"
  },
  "devDependencies": {
    "ava": "~0.19.1",
    "supertest": "~3.0.0",
    "tap-dot": "~1.0.5"
  },
  "engines": {
    "node": "4.3.2"
  }
}
